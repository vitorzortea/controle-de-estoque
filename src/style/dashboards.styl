.box-dash
  width 100%
  min-height 100%
  background-color #FBF9F8
  border solid 1px #D5C1B5
  border-radius 10px
  padding 20px

  h2
    font-weight bold
    font-size 20px

  .box-table
    overflow auto
    width 100%
    display block
    width calc(100% + 40px)
    margin 10px -20px 10px

  table
    width 100%
    border-spacing 0
    border-bottom 1px solid #d5c1b5
    th
      background-color #f4eae6
      font-weight bold
    td,th
      border-top 1px solid #d5c1b5
      padding 8px
      &:first-child
        padding-left 20px
      &:last-child
        padding-right 20px


